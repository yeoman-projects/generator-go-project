{
  "name": "@kyoh86/generator-go-project",
  "version": "1.4.13",
  "description": "A Yeoman generator to scaffold golang project.",
  "homepage": "https://github.com/kyoh86/generator-go-project",
  "author": {
    "name": "kyoh86",
    "email": "me@kyoh86.dev",
    "url": "https://kyoh86.dev"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "Yeoman",
    "go",
    "golang",
    "generator",
    "scaffold",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.36.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^6.0.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "inquirer-search-list": "^1.2.6",
    "yeoman-generator": "^5.2.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepare": "yarn audit",
    "prepublishOnly": "yarn audit",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/kyoh86/generator-go-project",
  "license": "MIT"
}
